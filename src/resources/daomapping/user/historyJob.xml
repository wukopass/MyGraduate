<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.rtworld.dao.user.IHistoryJobDao">
    <insert id="saveMsg" parameterType="historyJob">
        insert into historyjob(userid,companyname,department,jobname)
        values(#{userId},#{companyName},#{department},#{jobName});
    </insert>
    <update id="updateMsg" parameterType="historyJob">
        update historyjob set companyname=#{companyName},department=#{department},jobname=#{jobName}
    </update>
</mapper>